        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var daysInAdvance = gs.getProperty('x_843001_event_app.daysInAdvance');
    var dateToCheck = gs.daysAgo(-daysInAdvance);
    if (current.event_date < dateToCheck) {
        current.setAbortAction(true);
        gs.addErrorMessage("Incorrect: Entered date should be at least " + daysInAdvance + "days in the future");
    }

})(current, previous);]]></script>
