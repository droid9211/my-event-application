        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {

    // Modified the if to return if the newValue == oldValue to avoid
    // unecessary trips to the server
    if (isLoading || newValue === '' || newValue == oldValue) {
        return;
    }

    // Instantiate the GetEmailAddress Script Include 
    var getEmailAddr = new GlideAjax('GetEmailAddress');
    ajax.addParam('sysparm_name', 'getEmail');
    ajax.addParam('sysparm_userID', g_form.getValue('note_proposed_by')); // change variable name here
    ajax.getXML(doSomething);

    function doSomething(response) {
        var answer = response.responseXML.documentElement.getAttribute("answer");
        var answers = JSON.parse(answer);
        g_form.setValue('note_proposed_by_email', answers.email.toString()); // change variable name here
    }
}]]></script>
